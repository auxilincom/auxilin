    "extract-css-chunks-webpack-plugin": "4.5.6",
    "find-up": "4.1.0",
    "lodash": "4.17.15",
    "next": "9.0.2",
    "normalize.css": "8.0.1",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-import": "12.0.1",
    "postcss-nested": "4.1.2",
    "postcss-preset-env": "6.7.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
