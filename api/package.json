{
  "name": "koa-api-starter",
  "version": "0.6.3",
  "description": "Koa api",
  "private": false,
  "main": "src/app.js",
  "author": "Auxilin",
  "license": "MIT",
  "keywords": [
    "koa",
    "rest api",
    "auxilin"
  ],
  "scripts": {
    "build": "run-s build:**",
    "build:email": "mjml ./src/assets/emails/*.mjml -o ./src/assets/emails/dist/",
    "prestart": "npm run build",
    "start": "node src/app.js",
    "pretest": "npm run build",
    "test": "run-s test:**",
    "test:eslint": "eslint ./src",
    "test:mocha": "NODE_ENV=test mocha --timeout 20000 --recursive --exit -c -R spec 'src/**/*.spec.js'",
    "development": "NODE_ENV=development nodemon --watch src src/app.js",
    "format": "prettier-eslint --write \"src/**/*.js\""
  },
  "dependencies": {
    "@auxilin/common-logger": "0.5.1",
    "@auxilin/email-service": "0.4.1",
    "@auxilin/node-mongo": "1.3.0",
    "@hapi/joi": "15.1.1",
    "app-module-path": "2.2.0",
    "bcrypt": "3.0.6",
    "jsonwebtoken": "8.5.1",
    "kcors": "2.2.2",
    "koa": "2.8.1",
    "koa-bodyparser": "4.2.1",
    "koa-helmet": "5.1.0",
    "koa-logger": "3.2.1",
    "koa-mount": "4.0.0",
    "koa-router": "7.4.0",
    "lodash": "4.17.15",
    "nodemon": "1.19.2",
    "psl": "1.4.0",
    "redis": "2.8.0"
  },
  "devDependencies": {
    "@auxilin/eslint-config": "0.2.0",
    "babel-eslint": "10.0.3",
    "chai": "4.2.0",
    "eslint": "6.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "faker": "4.1.0",
    "mjml": "4.4.1",
    "mocha": "6.2.0",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "5.0.0",
    "supertest": "4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auxilincom/koa-api-starter.git"
  },
  "engines": {
    "node": ">= 10"
  }
}
